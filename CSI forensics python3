hair_color = "GCCAGTGCCG"
eye_color = "GGGAGGTGGC"
facial_shape = "GCCACGG"
race_color = "AAAACCTCA"
gender_male = "TGCAGGAACTTC"
database = ""

print("Wellcome to CSI Lab!")
print("Are you a robat? if no? Please use the below temporary username and password inorder to access the DNA database:")
print("username: smart")
print("password: ninja")

user_name = "smart"
my_password = "ninja"

question = input("Please type your user name: ")

if question == user_name:
    print("Username verfied!")

else:
    print("Invalid Username/Try again!")

question = input("Type your password here: ")
if question == my_password:
    print("You are successfully loggedin!")



else:
    print("Invalid Password/Try again!")


question = input("Copy and paste the DNA text here inorder the database finds the suspect:")

if question == database:
